<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <integer name="fragment_id_trigger">1</integer>
    <integer name="fragment_id_trigger_and_action_list">2</integer>
    <integer name="fragment_id_trigger_options">3</integer>
    <integer name="fragment_id_config_keymap_all">4</integer>
    <integer name="fragment_id_constraints_and_options">5</integer>
    <integer name="fragment_id_constraint_list">6</integer>
    <integer name="fragment_id_action_list">7</integer>
    <integer name="fragment_id_fingerprint_map_options">8</integer>

    <!-- The fragments that should be shown when configuring a keymap -->
    <integer-array name="config_keymap_fragments" tools:ignore="InconsistentArrays">
        <item>@integer/fragment_id_trigger</item>
        <item>@integer/fragment_id_action_list</item>
        <item>@integer/fragment_id_constraint_list</item>
        <item>@integer/fragment_id_trigger_options</item>
    </integer-array>

    <!-- The fragments that should be shown when configuring a keymap -->
    <integer-array name="config_fingerprint_map_fragments" tools:ignore="InconsistentArrays">
        <item>@integer/fragment_id_action_list</item>
        <item>@integer/fragment_id_constraint_list</item>
        <item>@integer/fragment_id_fingerprint_map_options</item>
    </integer-array>

</resources>